<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ApplicationIcon></ApplicationIcon>
    <NeutralLanguage></NeutralLanguage>
    <AssemblyVersion>1.0.7.0</AssemblyVersion>
    <FileVersion>1.0.7.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Pages\CharacterPage.xaml.cs" />
    <Compile Remove="Pages\ConnectedDevicePage.xaml.cs" />
    <Compile Remove="Pages\DeviceSettingPage.xaml.cs" />
    <Compile Remove="Pages\MainTabbedPage.xaml.cs" />
    <Compile Remove="Pages\ScanBluetoothPage.xaml.cs" />
    <Compile Remove="Pages\Views\ScanBluetoothView.xaml.cs" />
    <Compile Remove="ViewModels\TestTimeViewModel.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="Pages\CharacterPage.xaml" />
    <EmbeddedResource Remove="Pages\ConnectedDevicePage.xaml" />
    <EmbeddedResource Remove="Pages\DeviceSettingPage.xaml" />
    <EmbeddedResource Remove="Pages\MainTabbedPage.xaml" />
    <EmbeddedResource Remove="Pages\ScanBluetoothPage.xaml" />
    <EmbeddedResource Remove="Pages\Views\ScanBluetoothView.xaml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Acr.UserDialogs" Version="7.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
    <PackageReference Include="Plugin.BluetoothLE" Version="6.0.0-beta2" />
    <PackageReference Include="Plugin.Permissions" Version="2.2.1" />
    <PackageReference Include="Plugin.Share" Version="7.1.1" />
    <PackageReference Include="ReactiveUI" Version="8.0.0-alpha0117" />
    <PackageReference Include="sqlite-net-standard" Version="1.5.1" />
    <PackageReference Include="Syncfusion.Xamarin.SfDataGrid" Version="16.1.0.26" />
    <PackageReference Include="Syncfusion.Xamarin.SfGauge" Version="16.1.0.26" />
    <PackageReference Include="Syncfusion.Xamarin.SfTabView" Version="16.1.0.26" />
    <PackageReference Include="Xam.Plugins.Settings" Version="3.2.0.130-beta" />
    <PackageReference Include="Xamarin.Forms" Version="2.5.0.280555" />
  </ItemGroup>

  <ItemGroup>
    <Page Include="Pages\Views\ScanBleView.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="App.xaml.cs">
      <DependentUpon>*.xaml</DependentUpon>
    </Compile>
    <Compile Update="Pages\Views\CharacteristicView.xaml.cs">
      <DependentUpon>CharacteristicView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Pages\Views\DeviceSettingView.xaml.cs">
      <DependentUpon>DeviceSettingView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Resources\AppResource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AppResource.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Pages\SCUItemsPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Pages\SettingsLanguagePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Pages\Views\BluetoothSettingInfoView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Pages\Views\ScanBleDevicesView.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Pages\Views\ScanBluetoothView1.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\AppResource.en.resx">
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\AppResource.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>AppResource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>