<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SCUScanner.Pages.Views.BluetoothSettingInfoView"
             BackgroundColor="White">
  <ContentView.Content>
        <StackLayout Padding="10,10" Orientation="Vertical" >
            <Label Text="{Binding BlueToothTornOffText}"/>
            <Label/>
            <Label x:Name="lblHintBluetoothAndroidText" Text="{Binding Resources[SetHintBluetoothAndroidText]}">

            </Label>
            <Label x:Name="lblHintBluetoothIOSText"  Text="{Binding Resources[SetHintBluetoothIOSText]}">

            </Label>
            </StackLayout >
    </ContentView.Content>
</ContentView>